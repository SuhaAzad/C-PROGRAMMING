

#include <stdio.h>
struct student{

 int roll;
 char name[100];
 float marks;
};
int main()

{   struct student s[2];
    int i;
    printf("Enter the roll no name and marks of the student\n");
    for(i=0;i<2;i++)
        {
         scanf("%d%s%f",&s[i].roll,s[i].name,&s[i].marks);
    }

     for(i=0;i<2;i++)
     {
        printf("%d\t%s\t%f\t \n",s[i].roll,s[i].name,s[i].marks);
    }

        return 0;
}


